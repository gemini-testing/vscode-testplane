{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "out",
        "lib": ["ESNext"],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "moduleResolution": "Bundler"
    },
    "include": ["src"]
}
